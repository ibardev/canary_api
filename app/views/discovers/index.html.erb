<p id="notice"><%= notice %></p>

<h1>Listing Discovers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @discovers.each do |discover| %>
      <tr>
        <td><%= link_to 'Show', discover %></td>
        <td><%= link_to 'Edit', edit_discover_path(discover) %></td>
        <td><%= link_to 'Destroy', discover, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Discover', new_discover_path %>
